public class Main {
    public static void main(String[] args) {
        // Declaraciones de variables
        int SUMA = 0;
        int X = 20;
        
        // Operaciones
        SUMA = SUMA + X;
        int Y = 40;
        X = X + Y * Y;
        SUMA = SUMA + X / Y;
        
        // Impresi√≥n del resultado
        System.out.println("EL VALOR DE LA SUMA ES: " + SUMA);
    }
}
